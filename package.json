{
  "name": "ndla-frontend",
  "version": "0.1.16",
  "description": "NDLA Frontend",
  "scripts": {
    "pretest": "npm run lint",
    "clean": "rimraf ./htdocs/assets/*",
    "test": "cross-env NODE_ENV=unittest ava",
    "tdd": "npm test -- --watch",
    "copy-svg-polyfill": "cp node_modules/svg4everybody/dist/svg4everybody.min.js htdocs/assets/",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js -p --bail",
    "postbuild": "npm run copy-svg-polyfill",
    "start": "cross-env NODE_ENV=development nodemon --watch server server",
    "start-prod": "cross-env NODE_ENV=production node server",
    "lint": "eslint src server"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./src/__tests__/_initTestEnv"
    ]
  },
  "engines": {
    "node": "~6.6.0",
    "npm": "~3.10.3"
  },
  "devDependencies": {
    "ava": "0.16.0",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.3.1",
    "eslint-config-airbnb": "10.0.1",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.1.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "flux-standard-action": "0.6.1",
    "jsdom": "9.4.2",
    "json-loader": "0.5.4",
    "nock": "8.0.0",
    "nodemon": "1.10.2",
    "postcss-cssnext": "2.8.0",
    "postcss-focus": "1.0.0",
    "postcss-import": "8.1.2",
    "postcss-loader": "0.13.0",
    "postcss-reporter": "1.4.1",
    "react-addons-test-utils": "15.3.1",
    "redux-mock-store": "1.1.4",
    "sinon": "1.17.5",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-manifest-plugin": "1.0.1"
  },
  "dependencies": {
    "babel-polyfill": "6.13.0",
    "babel-register": "6.14.0",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "date-fns": "1.3.0",
    "defined": "1.0.0",
    "express": "4.14.0",
    "history": "2.1.2",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.15.0",
    "node-uuid": "1.4.7",
    "normalize-css": "2.3.1",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-helmet": "3.1.0",
    "react-intl": "2.1.5",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-actions": "0.11.0",
    "redux-saga": "0.11.1",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.3",
    "serialize-javascript": "1.3.0",
    "svg4everybody": "2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NDLANO/ndla-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/NDLANO/ndla-frontend/issues"
  },
  "homepage": "https://github.com/NDLANO/ndla-frontend",
  "author": "ndla@knowit.no",
  "license": "GPL-3.0"
}
